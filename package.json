{
  "name": "dinoapp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "PORT=3000 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=\"__tests__/(hooks|lib|components)\"",
    "test:integration": "jest --config jest.config.api.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:visual": "percy snapshot ./visual-tests",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "export-claude": "node scripts/export-claude.js",
    "db:generate": "prisma generate",
    "db:generate:dev": "prisma generate --schema=./prisma/schema.dev.prisma",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:push:dev": "prisma db push --schema=./prisma/schema.dev.prisma",
    "db:studio": "prisma studio",
    "db:studio:dev": "prisma studio --schema=./prisma/schema.dev.prisma",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-visa": "node lib/scripts/seed-visa-requirements.js",
    "db:optimize": "node scripts/performance/database-optimize.js optimize",
    "db:health": "node scripts/performance/database-optimize.js health",
    "db:indexes": "node scripts/performance/database-optimize.js indexes",
    "db:analyze": "node scripts/performance/database-optimize.js analyze",
    "deploy": "vercel",
    "deploy:preview": "vercel",
    "deploy:production": "vercel --prod",
    "deploy:docker": "bash scripts/deploy.sh docker",
    "deploy:railway": "bash scripts/deploy.sh railway",
    "deploy:netlify": "bash scripts/deploy.sh netlify",
    "predeploy": "npm run lint && npm run type-check && npm run build",
    "postdeploy": "echo 'Deployment completed!'",
    "postinstall": "prisma generate",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "vercel-build": "prisma generate && next build",
    "check:all": "npm run type-check && npm run lint && npm run test",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prepare": "husky || true",
    "pre-commit": "lint-staged",
    "ci:test": "npm run check:all && npm run build",
    "security:audit": "npm audit --audit-level=moderate",
    "lighthouse": "lighthouse http://localhost:3000 --budget-path=./lighthouse-budget.json",
    "bundle-size": "size-limit",
    "bundle-check": "node scripts/bundle-analysis.js",
    "backup:db": "node scripts/backup/backup-db.js",
    "backup:files": "node scripts/backup/backup-files.js",
    "backup:test-recovery": "node scripts/backup/test-recovery.js",
    "deploy:health-check": "node scripts/deployment/health-check.js",
    "deploy:monitor": "node scripts/deployment/monitor.js",
    "deploy:rollback": "node scripts/deployment/rollback.js",
    "deploy:rollback:dry-run": "node scripts/deployment/rollback.js --dry-run",
    "test:e2e:staging": "PLAYWRIGHT_BASE_URL=$STAGING_URL playwright test",
    "docs:generate": "node scripts/docs/generate-docs.js",
    "docs:quality": "node scripts/docs/doc-quality-check.js",
    "docs:quality:report": "node scripts/docs/doc-quality-check.js report-only",
    "docs:all": "npm run docs:generate && npm run docs:quality:report"
  },
  "keywords": [
    "travel",
    "digital-nomad",
    "visa",
    "schengen",
    "travel-manager",
    "nextjs",
    "typescript"
  ],
  "author": "DINO Team",
  "license": "MIT",
  "description": "디지털 노마드를 위한 스마트 여행 관리 플랫폼 - 78개국 비자 규정 추적, 셰겐 계산기, Google 서비스 통합",
  "dependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^9.46.0",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^20.19.11",
    "@types/node-cron": "^3.0.11",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.23.12",
    "googleapis": "^154.1.0",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-react": "^0.539.0",
    "next": "^14.2.31",
    "next-auth": "^4.24.11",
    "node-cron": "^4.2.1",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tar": "^7.4.3",
    "web-push": "^3.6.7",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.31",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.23",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.31",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
